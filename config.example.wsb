<Configuration>
	<MappedFolders>
		<MappedFolder>
			<HostFolder>C:\Users\Username\wsb</HostFolder>
			<SandboxFolder>C:\bootstrap</SandboxFolder>
			<ReadOnly>true</ReadOnly>
		</MappedFolder>
	</MappedFolders>
	<LogonCommand>
		<Command>powershell.exe -ExecutionPolicy Bypass -Command "start powershell.exe { -File C:\bootstrap\bootstrap.ps1 }"</Command>
	</LogonCommand>
</Configuration>
